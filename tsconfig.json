{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@admin/*": ["src/app/modules/admin/*"],
      "@admin": ["src/app/modules/admin"],
      "@services/*": ["src/app/services/*"],
      "@services": ["src/app/services"],
      "@user/*": ["src/app/modules/user/*"],
      "@user": ["src/app/modules/ser"]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"]
  }
}
